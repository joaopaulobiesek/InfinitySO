@model CompanyFormViewModel
<form asp-action="Register">
    <div class="row">
        <div class="col-lg-6 col-md-12">
            <div class="card ">
                <div class="card-header card-header-success card-header-text">
                    <div class="card-text">
                        <h4 class="card-title">Register student</h4>
                    </div>
                </div>
                <div class="card-body ">

                    <!--Start-->
                    <div class="text-danger">@ViewData["Result"]</div>
                    <div asp-validation-summary="All" class="text-danger"></div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Company.CorporateName"></label>
                            <input asp-for="Company.CorporateName" class="form-control" />
                            <span asp-validation-for="Company.CorporateName" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Company.FantasyName"></label>
                            <input asp-for="Company.FantasyName" class="form-control" />
                            <span asp-validation-for="Company.FantasyName" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Company.CNPJ"></label>
                            <input asp-for="Company.CNPJ" class="form-control" />
                            <span asp-validation-for="Company.CNPJ" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Company.StateRegistration"></label>
                            <input asp-for="Company.StateRegistration" class="form-control" />
                            <span asp-validation-for="Company.StateRegistration" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Company.Phone"></label>
                            <input asp-for="Company.Phone" class="form-control" />
                            <span asp-validation-for="Company.Phone" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Company.Cell"></label>
                            <input asp-for="Company.Cell" class="form-control" />
                            <span asp-validation-for="Company.Cell" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Company.Email"></label>
                            <input asp-for="Company.Email" class="form-control" />
                            <span asp-validation-for="Company.Email" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Company.Creation"></label><br />
                            <div class="input-group-prepend col-md-7">
                                <i class="material-icons">av_timer</i>
                                <input asp-for="Company.Creation" class="form-control" />
                            </div>
                            <span asp-validation-for="Company.Creation" class="text-danger"></span>
                        </div>
                    </div>
                    <!--End-->
                </div>
            </div>
        </div>

        <div class="col-lg-6 col-md-12">
            <div class="card ">
                <div class="card-header card-header-success card-header-text">
                    <div class="card-text">
                        <h4 class="card-title">Register address</h4>
                    </div>
                </div>
                <div class="card-body ">
                    <!--Start-->
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Address.State"></label>
                            <input asp-for="Address.State" class="form-control" />
                            <span asp-validation-for="Address.State" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Address.CEP"></label>
                            <input asp-for="Address.CEP" class="form-control" />
                            <span asp-validation-for="Address.CEP" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Address.City"></label>
                            <input asp-for="Address.City" class="form-control" />
                            <span asp-validation-for="Address.City" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Address.Neighborhood"></label>
                            <input asp-for="Address.Neighborhood" class="form-control" />
                            <span asp-validation-for="Address.Neighborhood" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Address.Street"></label>
                            <input asp-for="Address.Street" class="form-control" />
                            <span asp-validation-for="Address.Street" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Address.Number"></label>
                            <input asp-for="Address.Number" class="form-control" />
                            <span asp-validation-for="Address.Number" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-sm-7">
                        <div class="form-group">
                            <label asp-for="Address.Complement"></label>
                            <input asp-for="Address.Complement" class="form-control" />
                            <span asp-validation-for="Address.Complement" class="text-danger"></span>
                        </div>
                    </div><br /><hr />
                    <div class="form-group">
                        <input type="submit" value="Create" class="btn btn-info" />
                        <a asp-action="Index" class="btn btn-secondary" data-dismiss="modal">Back to List</a>
                    </div>
                    <!--End-->
                </div>
            </div>
        </div>
    </div>
</form>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}